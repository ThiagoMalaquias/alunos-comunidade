<p id="notice"><%= notice %></p>

<h1>Alunos</h1>

<table>
  <thead>
    <tr>
      <th>Callback type</th>
      <th>Hottok</th>
      <th>Aff</th>
      <th>Aff name</th>
      <th>Currency</th>
      <th>Xcode</th>
      <th>Payment type</th>
      <th>Payment engine</th>
      <th>Status</th>
      <th>Prod</th>
      <th>Prod name</th>
      <th>Producer name</th>
      <th>Purchase date</th>
      <th>Confirmation purchase date</th>
      <th>Original offer price</th>
      <th>Cms marketplace</th>
      <th>Cms vendor</th>
      <th>Price</th>
      <th>Full price</th>
      <th>Name subscription plan</th>
      <th>Email</th>
      <th>Name</th>
      <th>First name</th>
      <th>Last name</th>
      <th>Phone checkout local code</th>
      <th>Phone checkout number</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @alunos.each do |aluno| %>
      <tr>
        <td><%= aluno.callback_type %></td>
        <td><%= aluno.hottok %></td>
        <td><%= aluno.aff %></td>
        <td><%= aluno.aff_name %></td>
        <td><%= aluno.currency %></td>
        <td><%= aluno.xcode %></td>
        <td><%= aluno.payment_type %></td>
        <td><%= aluno.payment_engine %></td>
        <td><%= aluno.status %></td>
        <td><%= aluno.prod %></td>
        <td><%= aluno.prod_name %></td>
        <td><%= aluno.producer_name %></td>
        <td><%= aluno.purchase_date %></td>
        <td><%= aluno.confirmation_purchase_date %></td>
        <td><%= aluno.original_offer_price %></td>
        <td><%= aluno.cms_marketplace %></td>
        <td><%= aluno.cms_vendor %></td>
        <td><%= aluno.price %></td>
        <td><%= aluno.full_price %></td>
        <td><%= aluno.name_subscription_plan %></td>
        <td><%= aluno.email %></td>
        <td><%= aluno.name %></td>
        <td><%= aluno.first_name %></td>
        <td><%= aluno.last_name %></td>
        <td><%= aluno.phone_checkout_local_code %></td>
        <td><%= aluno.phone_checkout_number %></td>
        <td><%= link_to 'Show', aluno %></td>
        <td><%= link_to 'Edit', edit_aluno_path(aluno) %></td>
        <td><%= link_to 'Destroy', aluno, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Aluno', new_aluno_path %>
